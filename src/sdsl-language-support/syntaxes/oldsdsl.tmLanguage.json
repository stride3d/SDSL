{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "SDSL",
    "patterns": [
        {
            "include": "#comment"
        },
        {
            "include": "#preprocessors"
        },
        {
            "include": "#declarations"
        }
    ],
    "repository": {
        "declarations": {
            "patterns": [
                {
                    "include": "#namespace-declaration"
                },
                {
                    "include": "#shader-declaration"
                },
                {
                    "include": "#punctuation-semicolon"
                },
                {
                    "include": "#comment"
                }
            ]
        },
        "namespace-declaration": {
            "begin": "\\b(namespace)\\s+",
            "beginCaptures": {
                "1": {
                    "name": "keyword.other.namespace.sdsl"
                }
            },
            "end": "(?<=\\})|(?=;)",
            "patterns": [
                {
                    "include": "#comment"
                },
                {
                    "name": "entity.name.type.namespace.sdsl",
                    "match": "@?[_[:alpha:]][_[:alnum:]]*"
                },
                {
                    "include": "#punctuation-accessor"
                },
                {
                    "begin": "\\{",
                    "beginCaptures": {
                        "0": {
                            "name": "punctuation.curlybrace.open.sdsl"
                        }
                    },
                    "end": "\\}",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.curlybrace.close.sdsl"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#declarations"
                        },
                        {
                            "include": "#using-directive"
                        },
                        {
                            "include": "#punctuation-semicolon"
                        }
                    ]
                }
            ]
        },
        "type-declarations": {
            "patterns": [
                {
                    "include": "#preprocessor"
                },
                {
                    "include": "#comment"
                },
                {
                    "include": "#storage-modifier"
                },
                {
                    "include": "#shader-declaration"
                },
                {
                    "include": "#enum-declaration"
                },
                {
                    "include": "#attribute-section"
                },
                {
                    "include": "#punctuation-semicolon"
                }
            ]
        },
        "shader-declaration": {
            "begin": "(?=\\bshader\\b)",
            "end": "(?=})",
            "beginCaptures": {
                "1": {
                    "name": "keyword.other.shader.sdsl"
                }
            },
            "patterns": [
                {
                    "include": "#comment"
                },
                {
                    "patterns": [
                        {
                            "begin": "(shader)\\s*([_[:alpha:]][_[:alnum:]]*)",
                            "end": "(?=(\\<|:|\\{))",
                            "beginCaptures": {
                                "1": {
                                    "name": "keyword.other.shader.sdsl"
                                },
                                "2": {
                                    "name": "entity.name.class.shader.sdsl"
                                }
                            }
                        },
                        {
                            "name": "declaration.shader.generics.sdsl",
                            "begin": "\\<",
                            "end": "(?<=\\>)",
                            "patterns": [
                                {
                                    "include": "#punctuation.comma"
                                },
                                {
                                    "match": "([[:alnum:]]+)\\s+([_[:alpha:]][_[:alnum:]]*)",
                                    "captures": {
                                        "1": {
                                            "patterns": [
                                                {
                                                    "include": "#type-builtin"
                                                }
                                            ]
                                        },
                                        "2": {
                                            "name": "variable.name.mixin.generics.sdsl"
                                        }
                                    }
                                }
                            ]
                        },
                        {
                            "name": "declaration.shader.inherit.sdsl",
                            "begin": "(?<=:)",
                            "end": "(?=\\{)",
                            "patterns": [
                                {
                                    "include": "#comment"
                                },
                                {
                                    "include": "#preprocessor"
                                },
                                {
                                    "include": "#punctuation.comma"
                                },
                                {
                                    "match": "([_[:alpha:]][_[:alnum:]]*)(?!\\<)",
                                    "captures": {
                                        "1": {
                                            "patterns": [
                                                {
                                                    "include": "#type"
                                                }
                                            ]
                                        }
                                    }
                                },
                                {
                                    "begin": "([_[:alpha:]][_[:alnum:]]*)(?=\\<)",
                                    "beginCaptures": {
                                        "1": {
                                            "patterns": [
                                                {
                                                    "include": "#type"
                                                }
                                            ]
                                        }
                                    },
                                    "end": "(?<=\\>)",
                                    "patterns": [
                                        {
                                            "include": "#number"
                                        },
                                        {
                                            "name": "keyword.other",
                                            "match": "(true|false)"
                                        },
                                        {
                                            "include": "#punctuation.comma"
                                        },
                                        {
                                            "include": "#builtin-constructor"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "begin": "(?=\\{)",
                    "end": "(?=})",
                    "name": "declaration.shader.body",
                    "patterns": [
                        {
                            "include": "#shader-members"
                        }
                    ]
                },
                {
                    "include": "#comment"
                },
                {
                    "include": "#preprocessor"
                }
            ],
            "name": "declaration.shader.sdsl"
        },
        "shader-members": {
            "name": "shader.members.sdsl",
            "patterns": [
                {
                    "include": "#preprocessor"
                },
                {
                    "name": "shader.member.comment",
                    "include": "#comment"
                },
                {
                    "include": "#rgroup"
                },
                {
                    "include": "#cbuffer"
                },
                {
                    "include": "#field-declaration",
                    "name": "shader.member.field.declaration"
                },
                {
                    "include": "#method-declaration",
                    "name": "shader.member.method.declaration"
                }
            ]
        },
        "field-declaration": {
            "match": "(([[:alpha:]]+)\\s+)?(([[:alpha:]]+)\\s+)?([[:alnum:]\\<\\>]+)\\s+([_[:alpha:]][_[:alnum:]]*)\\s*(?:;)",
            "captures": {
                "2": {
                    "patterns": [
                        {
                            "include": "#storage-modifier"
                        }
                    ]
                },
                "4": {
                    "patterns": [
                        {
                            "include": "#storage-modifier"
                        }
                    ]
                },
                "5": {
                    "patterns": [
                        {
                            "include": "#type"
                        }
                    ]
                },
                "6": {
                    "name": "variable.name.sdsl"
                }
            },
            "name": "field.declaration.sdsl"
        },
        "struct-declaration": {
            "begin": "\\bstruct\\b)",
            "end": "(?<=\\})",
            "patterns": [
                {
                    "begin": "(struct)\\b\\s+\n(@?[_[:alpha:]][_[:alnum:]]*)",
                    "beginCaptures": {
                        "0": {
                            "name": "keyword.other.struct.sdsl"
                        },
                        "1": {
                            "name": "entity.name.type.struct.sdsl"
                        }
                    },
                    "end": "(?=\\{)",
                    "patterns": [
                        {
                            "include": "#comment"
                        },
                        {
                            "include": "#type-parameter-list"
                        },
                        {
                            "include": "#parenthesized-parameter-list"
                        },
                        {
                            "include": "#base-types"
                        },
                        {
                            "include": "#generic-constraints"
                        }
                    ]
                },
                {
                    "begin": "\\{",
                    "beginCaptures": {
                        "0": {
                            "name": "punctuation.curlybrace.open.sdsl"
                        }
                    },
                    "end": "\\}",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.curlybrace.close.sdsl"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#shader-members"
                        }
                    ]
                },
                {
                    "include": "#preprocessor"
                },
                {
                    "include": "#comment"
                }
            ]
        },
        "enum-declaration": {
            "begin": "(?=\\benum\\b)",
            "end": "(?<=\\})",
            "patterns": [
                {
                    "begin": "(?=enum)",
                    "end": "(?=\\{)",
                    "patterns": [
                        {
                            "include": "#comment"
                        },
                        {
                            "match": "(enum)\\s+(@?[_[:alpha:]][_[:alnum:]]*)",
                            "captures": {
                                "1": {
                                    "name": "keyword.other.enum.sdsl"
                                },
                                "2": {
                                    "name": "entity.name.type.enum.sdsl"
                                }
                            }
                        },
                        {
                            "begin": ":",
                            "beginCaptures": {
                                "0": {
                                    "name": "punctuation.separator.colon.sdsl"
                                }
                            },
                            "end": "(?=\\{)",
                            "patterns": [
                                {
                                    "include": "#type"
                                }
                            ]
                        }
                    ]
                },
                {
                    "begin": "\\{",
                    "beginCaptures": {
                        "0": {
                            "name": "punctuation.curlybrace.open.sdsl"
                        }
                    },
                    "end": "\\}",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.curlybrace.close.sdsl"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#preprocessor"
                        },
                        {
                            "include": "#comment"
                        },
                        {
                            "include": "#attribute-section"
                        },
                        {
                            "include": "#punctuation-comma"
                        },
                        {
                            "begin": "@?[_[:alpha:]][_[:alnum:]]*",
                            "beginCaptures": {
                                "0": {
                                    "name": "entity.name.variable.enum-member.sdsl"
                                }
                            },
                            "end": "(?=(,|\\}))",
                            "patterns": [
                                {
                                    "include": "#comment"
                                },
                                {
                                    "include": "#variable-initializer"
                                }
                            ]
                        }
                    ]
                },
                {
                    "include": "#preprocessor"
                },
                {
                    "include": "#comment"
                }
            ]
        },
        "cbuffer": {
            "name": "cbuffer.sdsl",
            "begin": "(cbuffer)\\s+([_[:alpha:]][_[:alnum:]\\.]*)\\s*{",
            "end": "(?:})",
            "beginCaptures": {
                "1": {
                    "name": "keyword.other.cbuffer"
                },
                "2": {
                    "name": "entity.name"
                }
            },
            "patterns": [
                {
                    "include": "#field-declaration"
                }
            ]
        },
        "rgroup": {
            "name" : "rgroup.block.sdsl",
            "begin": "(rgroup)",
            "end": "(?:})",
            "beginCaptures": {
                "1": {
                    "name": "keyword.other.rgroup"
                }
            },
            "patterns": [
                {
                    "name" : "rgroup.body.sdsl",
                    "begin": "{",
                    "end": "(?=})",
                    "patterns": [
                        {
                            "include": "#field-declaration"
                        }
                    ]
                },
                {
                    "name" : "entity.name.type.rgroup.name.sdsl",
                    "begin": "([_[:alnum:]]*)",
                    "end": "(?={)"
                }
                
            ]
        },
        "method-declaration": {
            "name": "declaration.shader.method.sdsl",
            "begin": "(([[:alpha:]]+)\\s+)?(([[:alpha:]]+)\\s+)?([[:alnum:]]+)\\s+([_[:alpha:]][_[:alnum:]]*)(?:(?!\\()\\s)*",
            "end": "(?:})",
            "beginCaptures": {
                "2": {
                    "patterns": [
                        {
                            "include": "#storage-modifier"
                        }
                    ]
                },
                "4": {
                    "patterns": [
                        {
                            "include": "#storage-modifier"
                        }
                    ]
                },
                "5": {
                    "patterns": [
                        {
                            "include": "#type"
                        }
                    ]
                },
                "6": {
                    "name": "entity.name.function"
                }
            },
            "patterns": [
                {
                    "name": "declaration.shader.method.parameters",
                    "begin": "\\(",
                    "end": "\\)",
                    "patterns": [
                        {
                            "include": "#parameters-declaration"
                        }
                    ]
                },
                {
                    "name": "declaration.shader.method.body.sdsl",
                    "begin": "{",
                    "end": "(?=})",
                    "patterns": [
                        {
                            "include": "#statements"
                        },
                        {
                            "include": "#comment"
                        }
                    ]
                }
            ]
        },
        "statements": {
            "patterns": [
                {
                    "include": "#foreach"
                },
                {
                    "include": "#for"
                },
                {
                    "include": "#while"
                },
                {
                    "include": "#method-call"
                },
                {
                    "include": "#block"
                },
                {
                    "name" : "simple",
                    "begin": "(?=([_[:alnum:]]|\\+\\+|--))",
                    "end": "(?=;)",
                    "patterns": [
                        {
                            "include": "#method-call"
                        },
                        {
                            "include": "#return"
                        },
                        {
                            "include": "#number"
                        },
                        {
                            "include": "#booleans"
                        },
                        {
                            "include": "#declare"
                        },
                        {
                            "include": "#accessed"
                        },
                        
                        {
                            "include": "#access"
                        } 
                    ]
                }
            ]
        },
        "access-method" : {
            "name": "access",
            "begin": "(([_[:alnum:]]+\\s*\\(.\\s*)?)+)([_[:alnum:]]+)\\s*(?=()",
            "end": "(?=\\))",
            "beginCaptures": {
                "1" : {
                    "name" : "variable.name"
                },
                "2" : {
                    "name" : "entity.name.method.sdsl"
                }
            }
        },
        "accessed" : {
            "name": "access",
            "match": "([_[:alpha:]][_[:alnum:]]*)\\s*(?=\\.)",
            "captures": {
                "1" : {
                    "name" : "variable.name"
                }
            }
        },
        "access" : {
            "name": "access",
            "match": "\\.\\s*([_[:alpha:]][_[:alnum:]]*)\\s*(?!(\\(|[[:alnum:]]))",
            "captures": {
                "1" : {
                    "name" : "variable.name"
                }
            }
        },
        "declare": {
            "name": "declare",
            "match": "([_[:alpha:]][_[:alnum:]]*)\\s+([_[:alpha:]][_[:alnum:]]*)",
            "captures": {
                "1": {
                    "patterns": [
                        {
                            "include": "#type"
                        }
                    ]
                },
                "2": {
                    "name": "variable.name.sdsl"
                }
            }
        },
        "block": {
            "name": "block",
            "begin": "(?:{)",
            "end": "(?:})",
            "patterns": [
                {
                    "include": "#statements"
                }
            ]
        },
        "return": {
            "name": "return",
            "begin": "(return)\\s*",
            "end": "(?=;)",
            "beginCaptures": {
                "1": {
                    "name": "keyword.control.return"
                }
            },
            "patterns": [
                {
                    "include": "#statements"
                }
            ]
        },
        "while": {
            "name": "while",
            "begin": "(while)\\s*\\(",
            "end": "(?=\\))",
            "beginCaptures": {
                "1": {
                    "name": "keyword.control.while"
                }
            },
            "patterns": [
                {
                    "include": "#statements"
                },
                {
                    "name": "variable.name.sdsl",
                    "match": "([_[:alpha:]][_[:alnum:]]*)"
                }
            ]
        },
        "for": {
            "name": "for",
            "begin": "(for)\\s*\\(",
            "end": "(?=\\))",
            "beginCaptures": {
                "1": {
                    "name": "keyword.control.for"
                }
            },
            "patterns": [
                {
                    "include": "#declare"
                },
                {
                    "name": "variable.name.sdsl",
                    "match": "([_[:alpha:]][_[:alnum:]]*)"
                },
                {
                    "include": "#number"
                },
                {
                    "include": "#booleans"
                }
            ]
        },
        "foreach": {
            "name": "foreach",
            "match": "(foreach)\\s*\\(([_[:alpha:]][_[:alnum:]]*)\\s+([_[:alpha:]][_[:alnum:]]*)\\s+(in)\\s+([_[:alpha:]][_[:alnum:]]*)\\s*\\)",
            "captures": {
                "1": {
                    "name": "keyword.control.foreach"
                },
                "2": {
                    "patterns": [
                        {
                            "include": "#type"
                        }
                    ]
                },
                "3": {
                    "name": "variable.name.sdsl"
                },
                "4": {
                    "name": "keyword.control.in"
                },
                "5": {
                    "name": "variable.name.sdsl"
                }
            }
        },
        "base-types": {
            "name": "entity.other.inherited-class",
            "patterns": [
                {
                    "include": "#comment"
                },
                {
                    "include": "#type"
                },
                {
                    "include": "#mixin-name-generics-values"
                },
                {
                    "include": "#punctuation-comma"
                },
                {
                    "include": "#preprocessor"
                }
            ]
        },
        "type": {
            "name": "meta.type.sdsl",
            "patterns": [
                {
                    "match": "var",
                    "name": "keyword.other.var"
                },
                {
                    "include": "#type-builtin"
                },
                {
                    "include": "#type-buffer"
                },
                {
                    "include": "#type-name"
                }
            ]
        },
        "mixin-name-generics-declaration": {
            "name": "mixin.name.generics.declaration.sdsl",
            "begin": "([_[:alpha:]][_[:alnum:]*])\\s*\\<",
            "end": "(?<=\\>)",
            "beginCaptures": {
                "1": {
                    "name": "entity.name.type.sdsl"
                }
            },
            "patterns": [
                {
                    "include": "#parameters-declaration"
                }
            ]
        },
        "mixin-name-generics-values": {
            "name": "mixin.name.generics.value.sdsl",
            "begin": "([_[:alpha:]][_[:alnum:]]*)\\s*(\\<)",
            "end": "(\\>)",
            "beginCaptures": {
                "1": {
                    "name": "entity.name.type.sdsl"
                }
            },
            "patterns": [
                {
                    "include": "#number"
                },
                {
                    "name": "keyword.other",
                    "match": "(true|false)"
                },
                {
                    "include": "#punctuation.comma"
                },
                {
                    "include": "#builtin-constructor"
                }
            ]
        },
        "mixin-generics": {
            "begin": "\\<",
            "end": "(?<=\\>)",
            "patterns": [
                {
                    "include": "#number"
                },
                {
                    "name": "keyword.other",
                    "match": "(true|false)"
                },
                {
                    "include": "#punctuation.comma"
                },
                {
                    "include": "#builtin-constructor"
                }
            ]
        },
        "parameters-values": {
            "name": "paramters.values.sdsl",
            "patterns": [
                {
                    "include": "#number"
                },
                {
                    "name": "keyword.other",
                    "match": "(true|false)"
                },
                {
                    "include": "#punctuation.comma"
                },
                {
                    "include": "#builtin-constructor"
                },
                {
                    "include": "#method-call"
                },
                {
                    "include": "#accessed"
                },
                {
                    "include": "#access"
                },
                {
                    "name" : "variable.name.parameter.value.sdsl",
                    "match": "[_[:alpha:]][_[:alnum:]]*"
                }
            ]
        },
        "parameters-declaration": {
            "name": "parameters.declaration.sdsl",
            "patterns": [
                {
                    "include": "#punctuation.comma"
                },
                {
                    "match": "([[:alnum:]]+)\\s+([_[:alpha:]][_[:alnum:]]*)",
                    "captures": {
                        "1": {
                            "patterns": [
                                {
                                    "include": "#type-builtin"
                                }
                            ]
                        },
                        "2": {
                            "name": "variable.name.mixin.generics.sdsl"
                        }
                    }
                }
            ]
        },
        "mixin-generics-declaration": {
            "name": "mixin.generics.declaration.sdsl",
            "patterns": [
                {
                    "include": "#punctuation.comma"
                },
                {
                    "match": "([[:alnum:]]+)\\s+([_[:alpha:]][_[:alnum:]]*)",
                    "captures": {
                        "1": {
                            "patterns": [
                                {
                                    "include": "#type-builtin"
                                }
                            ]
                        },
                        "2": {
                            "name": "variable.name.mixin.generics.sdsl"
                        }
                    }
                }
            ]
        },
        "type-name": {
            "patterns": [
                {
                    "match": "(@?[_[:alpha:]][_[:alnum:]]*)\\s*(\\:\\:)",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.alias.sdsl"
                        },
                        "2": {
                            "name": "punctuation.separator.coloncolon.sdsl"
                        }
                    }
                },
                {
                    "match": "(@?[_[:alpha:]][_[:alnum:]]*)\\s*(\\.)",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.sdsl"
                        },
                        "2": {
                            "name": "punctuation.accessor.sdsl"
                        }
                    }
                },
                {
                    "match": "(\\.)\\s*(@?[_[:alpha:]][_[:alnum:]]*)",
                    "captures": {
                        "1": {
                            "name": "punctuation.accessor.sdsl"
                        },
                        "2": {
                            "name": "entity.name.type.sdsl"
                        }
                    }
                },
                {
                    "name": "entity.name.type.sdsl",
                    "match": "@?[_[:alpha:]][_[:alnum:]]*"
                }
            ]
        },
        "type-builtin": {
            "match": "\\b((bool|s?byte|u?short|u?int|u?long|float|double)([2-4]?|([2-4]x[2-4])?)|void)\\b",
            "captures": {
                "1": {
                    "name": "keyword.type.sdsl"
                }
            }
        },
        "type-buffer": {
            "name" : "buffer",
            "begin": "(Buffer)\\s*\\<",
            "end": "(?=\\>)",
            "captures": {
                "1" : {"name" : "entity.name.type"}
            },
            "patterns": [
                {"include": "#type-builtin"}
            ]
        },
        "comment": {
            "patterns": [
                {
                    "name": "comment.block.sdsl",
                    "begin": "/\\*",
                    "beginCaptures": {
                        "0": {
                            "name": "punctuation.definition.comment.sdsl"
                        }
                    },
                    "end": "\\*/",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.definition.comment.sdsl"
                        }
                    }
                },
                {
                    "name" : "comment.block.documentation",
                    "begin": "(^\\s+)?(?=///)",
                    "end": "(?=$)",
                    "beginCaptures": {
                        "2": {
                            "name": "punctuation.whitespace.comment.leading.sdsl"
                        }
                    }
                },
                {
                    "begin": "(^\\s+)?(?=//)",
